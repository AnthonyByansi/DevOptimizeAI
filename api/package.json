    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "@types/node-fetch": "^2.6.3",
    "env-cmd": "^10.1.0",
    "typescript": "^4.4.4"
